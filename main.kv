<BasicCalculator>:
    name: 'basic'
    id: calculator
    display: entry
    historyID: history


    Screen:
        MDNavigationLayout:
            ScreenManager:
                Screen:
                    BoxLayout:
                        orientation: 'vertical'
                        MDTopAppBar:
                            title: "Basic Calculator"
                            pos_hint: {'top':1}
                            left_action_items: [['menu', lambda x: nav_drawer.set_state('toggle')]]
                            right_action_items: [['gamepad-variant', lambda x: app.show_rgb_dialog()]]
                            elevation: 3
                            md_bg_color: "#b2173a"

                        MDFloatLayout:
                            canvas.before:
                                Color:
                                    rgba: .11, .11, .086, 1
                                RoundedRectangle:
                                    pos: root.width*.05, root.height*.01
                                    size: root.width*.90, root.height*.87
                                    radius: [25, 25, 25, 25]

                            GridLayout:
                                rows:2
                                pos: root.width*.05, root.height*.025
                                padding: dp(20), dp(50), dp(20), dp(10)
                                size_hint: .90, 1
                                TextInput:
                                    id: entry
                                    font_size: sp(self.height*0.70)
                                    size_hint: 1, 0.20
                                    multiline: False
                                    readonly: True
                                    padding: sp(self.width*0.025), sp(self.height*0.1), sp(self.width*0.025), sp(self.height*0.07)
                                    spacing: sp(self.height*0.70)
                                    base_direction: "rtl"
                                    allow_copy: False
                                    cursor_blink: False
                                    cursor_color: 1, 1, 1, 1

                                GridLayout:
                                    rows:5
                                    spacing: dp(-15)
                                    BoxLayout:
                                        pos_hint: {'center_y':0.5}
                                        spacing: dp(10)
                                        adoptive_size: True

                                        Button:
                                            font_size: sp(30)
                                            text: "AC"
                                            on_press: entry.text = ""
                                            size_hint: .25, .75
                                            theme_text_color: "Custom"
                                            text_color: 1, 1, 1, 1
                                            line_color: 0, 0, 0, 0
                                            background_color: .51 , .51, .51, 1


                                        Button:
                                            font_size: sp(30)
                                            size_hint: .25, .75
                                            theme_text_color: "Custom"
                                            text_color: 1, 1, 1, 1
                                            line_color: 0, 0, 0, 0
                                            background_color: .51 , .51, .51, 1
                                            on_press:
                                                list = entry.text[:-1]
                                                entry.text = list
                                            Image:
                                                source:"backspace.png"
                                                size: dp(30), dp(30)
                                                keep_ratio: False
                                                center_x: self.parent.center_x
                                                center_y: self.parent.center_y


                                        Button:
                                            font_size: sp(30)
                                            text: "%"
                                            on_press:entry.text += self.text
                                            size_hint: .25, .75
                                            theme_text_color: "Custom"
                                            text_color: 1, 1, 1, 1
                                            line_color: 0, 0, 0, 0
                                            background_color: .51 , .51, .51, 1

                                        Button:
                                            font_size: sp(30)
                                            text: "/"
                                            on_press:entry.text += self.text
                                            size_hint: .25, .75
                                            theme_text_color: "Custom"
                                            text_color: 1, 1, 1, 1
                                            line_color: 0, 0, 0, 0
                                            background_color: .51 , .51, .51, 1

                                    BoxLayout:
                                        pos_hint: {'center_y':0.5}
                                        spacing: dp(10)
                                        adoptive_size: True

                                        Button:
                                            size_hint: .25, .75
                                            theme_text_color: "Custom"
                                            text_color: 1, 1, 1, 1
                                            line_color: 0, 0, 0, 0
                                            background_color: 1 , 1, 1, 1
                                            font_size: sp(30)
                                            text: "7"
                                            on_press:entry.text += self.text

                                        Button:
                                            size_hint: .25, .75
                                            theme_text_color: "Custom"
                                            text_color: 1, 1, 1, 1
                                            line_color: 0, 0, 0, 0
                                            background_color: 1, 1, 1, 1
                                            font_size: sp(30)
