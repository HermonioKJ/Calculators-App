<BMI>
    name: 'bmi'
    Screen:
        MDNavigationLayout:
            ScreenManager:
                Screen:
                    BoxLayout:
                        orientation: 'vertical'
                        MDTopAppBar:
                            title: "BMI Calculator"
                            pos_hint: {'top':1}
                            left_action_items: [['menu', lambda x: nav_drawer.set_state('toggle')]]
                            right_action_items: [['gamepad-variant', lambda x: app.show_rgb_dialog()]]
                            elevation: 3
                            md_bg_color: "#b2173a"
                        MDFloatLayout:
                            canvas.before:
                                Color:
                                    rgba: .11, .11, .086, 1
                                RoundedRectangle:
                                    pos: root.width*.15, root.height*.30
                                    size: root.width*.70, root.height*.40
                                    radius: [25, 25, 25, 25]
                            FloatLayout:
                                orientation: 'vertical'
                                Image:
                                    pos_hint: {'center_x':.5, 'center_y':0.60}
                                    source: "team.png"
                                MDLabel:
                                    pos_hint: {'center_x':.5, 'center_y':0.40}
                                    text: 'Under construction'
                                    halign: 'center'
            MDNavigationDrawer:
                id: nav_drawer
                MDBoxLayout:
                    orientation: 'vertical'
                    padding: 0, 50, 0, 0
                    Image:
                        source: 'logo.png'
                        size_hint: 0.5, 0.5
                        pos_hint: {'center_x':.5, 'center_y':0.35}
                    MDLabel:
                        text: 'Swiss Calculator'
                        size_hint_y: None
                        height: self.texture_size[1]
                        font_size: 30
                        halign: 'center'
                    MDLabel:
                        text: 'Created by Hermonio, Leguira, Langreo'
                        size_hint_y: None
                        height: self.texture_size[1]
                        halign: 'center'
                        font_size: 13
                    MDLabel:
                        text: 'Version 1.0'
                        size_hint_y: None
                        height: self.texture_size[1]
                        halign: 'center'
                        font_size: 13
                    MDLabel:
                        text: ' '
                        size_hint_y: None
                        height: self.texture_size[1]
                        halign: 'center'
                        font_size: 20
                    ScrollView:
                        MDList:
                            OneLineIconListItem:
                                text: "Basic Calculator"
                                on_press:
                                    nav_drawer.set_state('toggle')
                                    root.manager.current = 'basic'
                                IconLeftWidget:
                                    icon: "calculator"
                            OneLineIconListItem:
                                text: "Quadratic Calculator"
                                on_press:
                                    nav_drawer.set_state('toggle')
                                    root.manager.current = 'quad'
                                IconLeftWidget:
                                    icon: "multiplication-box"
                            OneLineIconListItem:
                                text: "BMI Calculator"
                                disabled: True
                                IconLeftWidget:
                                    icon: "weight"
                            OneLineIconListItem:
                                text: "Home"
                                on_press:
                                    nav_drawer.set_state('toggle')
                                    root.manager.current = 'home'
                                IconLeftWidget:
                                    icon: "home"
                    MDLabel:
                        height: self.texture_size[1]
                        halign: "center"
                        text: "More calculators coming soon."
